<h1>Hello <%= current_user.email %> we love you.</h1>
<br>
<h4>Your Reservations:</h4>
<br>
<% @reservations.each do |reservation| %>
  Studio:
  <%= reservation.studio.name %>
<br>
  Starts on:
  <%= reservation.start_on %>
<br>
  Ends on:
  <%= reservation.end_on %>
<br>
  Approved:
    <% if reservation.approved %>
  Yes
    <% else %>
  Pending
  <% end %>
  <br>
  <br>
<% end %>
