<%= simple_form_for @studio do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :size %>
     <%= f.input :equipment %>
    <%= f.input :location %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Post your Studio' %>
  </div>
<% end %>
